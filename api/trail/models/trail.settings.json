{
  "kind": "collectionType",
  "collectionName": "trails",
  "info": {
    "name": "trail",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "trails"
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "geojson": {
      "type": "json"
    },
    "difficulty": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "distance": {
      "type": "decimal"
    },
    "elevation": {
      "type": "integer"
    },
    "rating": {
      "type": "integer",
      "max": 5,
      "min": 1,
      "required": false,
      "default": 1
    },
    "trailLength": {
      "type": "json"
    }
  }
}
